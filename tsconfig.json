        "outDir": "./dist",
        "target": "es2021",
        "module": "CommonJS",
        "moduleResolution": "node",
        "isolatedModules": true,
        "esModuleInterop": true,
        "noEmit": true,
        "declaration": false,
        "emitDeclarationOnly": false
